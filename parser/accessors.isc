accessor_group: 
    '$' NUMBER
    data: %2;
;
accessor_element: 
    '%' NUMBER
    data: %2;
;
accessor_char: 
    '^' NUMBER ('+' | '-')?
    data: 
        val: %2
        sign: %3
    ;
;
accessor_all: 
    accessor_group | accessor_element | accessor_char
    data: %1;
;
accessor:
    accessor_all ('>' accessor_all)* 
    data: 
        first: %1
        second: %3
    ;
;