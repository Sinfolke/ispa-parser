
cll_csupport_types: 
    ('unsigned'? ('char' | 'short' | 'int' | 'long' | ('long' 'long') ))
        |
    ('float' | 'double' | ('long' 'double'))

    data:
        type: %1
        templ: %2
    ;
;
cll_type:
    cll_csupport_types | #abstract

    data: %1;
    
    #abstract:
        ( 'var' | 'any' | 'num' | 'bool' | 'str' | ('arr' cll_template) | ('obj' cll_template) )
        data:
            type: %1
            templ: %2
        ;
    ;
;