OP:
    '+' | '-' | '*' | '/' | '%' | 
    // bitwise
    '&' | '|' | '^' | '<<' | '>>'
    data: %1;
;
ASSIGNMENT_OP:
    OP? '='

    data: %1;
;
COMPARE_OP:
    '==' | '!=' | '>' | '<' | '>=' | '<='

    data: %1;
;

LOGICAL_OP:
    LOGICAL_AND | LOGICAL_OR
    data: %1;
;
LOGICAL_NOT:
    '!' | 'not'
;
LOGICAL_AND:
    '&&' | 'and'
;
LOGICAL_OR:
    '||' | 'or'
;