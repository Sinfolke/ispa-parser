STRING:
    ('\'' @ (('\\' . ) | [^'] )* '\'')

    {@}
;
NUMBER:

    @ [+-]? @ ( [0-9]+ ) @ ([.,] [0-9]+)?
    
    @{sign, main, dec}
;
BOOLEAN:
    &d('true' | 'false')

    {d}
;
array: 

    '[' ( ( @ any_data ',')* )? ']'

    {@}

object:
    '{'
        ( @ ID | NUMBER ':' @ any_data ( ',' @ ID ':' @ any_data )* ';' )?
    '}'
    @{key, value, keys, values}
any_data: 

    @ BOOLEAN | NUMBER | STRING | ID | AT | array | object
    
    {@}
;
